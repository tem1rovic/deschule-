{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "container/*": ["container/*"],
      "static/*": ["static/*"],
      "styles/*": ["styles/*"],
      "layouts/*": ["layouts/*"],
      "pages/*": ["pages/*"],
      "helpers/*": ["helpers/*"],
      "hooks/*": ["hooks/*"],
      "store/*": ["store/*"],
      "models/*": ["models/*"],
      "services/*": ["services/*"],
      "types/*": ["types/*"],
      "utils/*": ["utils/*"],
      "languages/*": ["languages/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
